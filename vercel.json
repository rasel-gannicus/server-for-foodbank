{
    "version": 2,

    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
        "headers": [
          {
            "source": "http://localhost:5173",
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ]
          }
        ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
